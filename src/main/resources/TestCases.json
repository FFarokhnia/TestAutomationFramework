{
  "tests": [
    {
      "method": "POST",
      "description": "Create a new post",
      "params": {
        "baseUrl": "https://jsonplaceholder.typicode.com",
        "endpoint": "/posts",
        "headers": {
          "Content-Type": "application/json"
        },
        "request_body": {
          "title": "foo",
          "body": "bar",
          "userId": 1
        },
        "expected_status": 201,
        "expected_response_body": {
          "title": "foo",
          "body": "bar",
          "userId": 1
        }
      }
    },
    {
      "method": "POST",
      "description": "Create a new post",
      "params": {
        "baseUrl": "https://jsonplaceholder.typicode.com",
        "endpoint": "/posts",
        "headers": {
          "Content-Type": "application/json"
        },
        "request_body": {
          "title": "mahsa",
          "body": "farokh",
          "userId": 2
        },
        "expected_status": 201,
        "expected_response_body": {
          "title": "mahsa",
          "body": "farokh",
          "userId": 2
        }
      }
    }
  ]
}
